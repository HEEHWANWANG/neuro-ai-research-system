  "key_findings": {
    "finding_1": {
      "title": "Brain-JEPA Superior for fMRI",
      "description": "Representation-level predictive learning outperforms pixel-level reconstruction by 2-3% on fMRI tasks",
      "evidence": "fMRI's inherent noise (SNR 0.5-1.0) makes pixel-level targets unstable; representation-level predictions handle noise better",
      "implication": "Future improvements should prioritize JEPA-style approaches",
      "impact_level": "CRITICAL"
    },
    "finding_2": {
      "title": "Diversity > Scale",
      "description": "Multiple datasets provide better generalization than single-source scale",
      "evidence": "Brain-JEPA (3K hours, mixed sources) ≈ BrainLM (40K hours, single source); SwiFT v2 multi-cohort generalizes better",
      "implication": "Strategic data curation more important than raw quantity",
      "impact_level": "CRITICAL"
    },
    "finding_3": {
      "title": "Temporal Dynamics Underexplored",
      "description": "Current approaches don't optimize for temporal coherence despite BOLD carrying temporal information",
      "evidence": "BOLD has ~2-3 second autocorrelation carrying diagnostic information; current approaches use random masking",
      "implication": "Specialized temporal components (RNNs, dilated convolutions) remain unexplored",
      "impact_level": "HIGH"
    },
    "finding_4": {
      "title": "Architectural Saturation Near 800M Parameters",
      "description": "Diminishing returns beyond 800M parameters indicate current approaches near theoretical limits",
      "evidence": "200M → 800M: clear gains; 800M → 3.2B: diminishing returns",
      "implication": "Future improvements from objective/architecture innovation, not just scaling",
      "impact_level": "MEDIUM"
    },
    "finding_5": {
      "title": "Clinical Translation Bottleneck",
      "description": "Research-grade accuracy (70-78%) insufficient for clinical deployment",
      "evidence": "Current approaches lack ~85% threshold needed for clinical use; missing uncertainty quantification",
      "implication": "Path to clinical use requires safety/explainability work alongside accuracy improvements",
      "impact_level": "HIGH"
    }
  },
